@article{KLpi0gg_ChPT_p6_VMD,
  title    = {Vector meson exchange contributions to $K\to\pi\gamma\gamma$ and $K_L\to\gamma \ell^+ \ell^-$},
  journal  = {Nuclear Physics B},
  volume   = {492},
  number   = {1},
  pages    = {417 - 451},
  year     = {1997},
  issn     = {0550-3213},
  doi      = {https://doi.org/10.1016/S0550-3213(97)80040-5},
  url      = {http://www.sciencedirect.com/science/article/pii/S0550321397800405},
  author   = {Giancarlo D'Ambrosio and Jorge Portolés},
  keywords = {Radiative non-leptonic kaon decays, Non-leptonic weak Hamiltonian, Chiral perturbation theory, Vector meson dominance},
  abstract = {We have studied in the framework of Chiral Perturbation Theory (χPT) the O(p6) vector meson contributions to K → πγγ and KL → γγ∗. We construct the most general χPT Lagrangian for the weak vector-pseudoscalar-photon (VPγ) vertex for K → πγγ and KL → γγ∗ and consequently we get the full structure of the O(p6) local contributions generated by vector meson exchange. We then compute new factorizable contributions to the weak VPγ vertex generated by the odd-intrinsic parity violating Lagrangian with no additional couplings. We find in fact a very nice agreement with the phenomenology of K → πγγ and KL → γγ∗, more predictive power and a deeper understanding of the O(p6) local operators. A novel interpretation of the KL → γγ∗ data is given. Also a comparison with the existing models is presented.}
}

