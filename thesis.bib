@article{IB,
  title    = {A new cylindrical photon-veto detector for the $K_L^0 \to \pi^0 \nu \overline{\nu}$ experiment},
  journal  = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
  volume   = {953},
  pages    = {163255},
  year     = {2020},
  issn     = {0168-9002},
  doi      = {https://doi.org/10.1016/j.nima.2019.163255},
  url      = {http://www.sciencedirect.com/science/article/pii/S0168900219315268},
