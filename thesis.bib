@article{ChPT_VMD_kaon,
  title    = {Vector meson exchange in radiative kaon decays and chiral perturbation theory},
  journal  = {Physics Letters B},
  volume   = {237},
  number   = {3},
  pages    = {481 - 487},
  year     = {1990},
